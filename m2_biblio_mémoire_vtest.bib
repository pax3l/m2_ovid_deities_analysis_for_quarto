@article{noauthor_femme_2001,
	title = {La femme dans les {Métamorphoses} d’{Apulée} : une descente dans l'animalité ?},
	volume = {33-34},
	shorttitle = {La femme dans les {Métamorphoses} d’{Apulée}},
	url = {https://sciencepress.mnhn.fr/fr/periodiques/anthropozoologica/33-34/la-femme-dans-les-metamorphoses-d-apulee-une-descente-dans-l-animalite},
	abstract = {Les Métamorphoses d\&\#39;Apulée formulent un véritable questionnement sur la condition humaine qui porte en particulier sur la place que doit occuper l\&\#39;être humain par rapport à l\&\#39;animal et à la divinité dans l\&\#39;univers. La réflexion sur la notion d\&\#39;animalité dans le comportement humain se développe à la fois dans la narration principale, par la métamorphose du héros-narrateur Lucius en âne, et dans la plupart des récits métadiégétiques où la relation amoureuse est en jeu et où la femme acquiert un statut de puissance funeste. Ce n\&\#39;est pas par l\&\#39;emploi d\&\#39;un champ lexical concernant l\&\#39;animal, mais par le recours à des mythes que le rapport entre la femme et l\&\#39;animal se met en place. En encadrant la métamorphose de Lucius en âne par le mythe de Diane et d\&\#39;Actéon et par le mythe de Psyché et de Cupidon, Apulée pressent et exploite l\&\#39;existence du lien entre violence, mort et sexualité, et fait d\&\#39;un certain type de personnage féminin - la magicienne - le symbole de l\&\#39;instinct incontrôlé qui entraîne fatalement les hommes à leur perte en les tirant vers la sphère de l\&\#39;animalité. De même qu\&\#39;Actéon, dans son désir coupable de voyeur, est transformé en l\&\#39;animal qu\&\#39;il portait déjà en lui, de même Lucius est métamorphosé en l\&\#39;animal priapique par excellence et devient l\&\#39;objet du désir féminin. Refusant de s\&\#39;accoupler avec une femme dans l\&\#39;amphithéâtre de Corinthe, il finit par refuser la confusion de l\&\#39;humain et de l\&\#39;animal - comble de l\&\#39;infamie -, et choisit de secouer le joug de sa condition servile d\&\#39;animal et de reconquérir la pleine souveraineté de soi sur soi. Mais cette chute dans l\&\#39;animalité a été nécessaire pour lui permettre de s\&\#39;élever vers la sphère du divin et d\&\#39;embrasser à la fin de l\&\#39;oeuvre la religion isiaque. Apulée se situe encore dans la lignée de la philosophie platonicienne en plaçant le héros face à lui-même et à la recherche de sa vérité qui passe par une différenciation entre l\&\#39;humain et l\&\#39;animal.},
	language = {fr},
	urldate = {2023-10-22},
	journal = {Anthropozoologica},
	year = {2001},
	keywords = {notion},
	pages = {85--92},
	file = {Snapshot:/Users/axelle/Zotero/storage/YKKR892L/la-femme-dans-les-metamorphoses-d-apulee-une-descente-dans-l-animalite.html:text/html},
}

