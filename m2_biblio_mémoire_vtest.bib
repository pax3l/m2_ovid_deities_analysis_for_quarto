@phdthesis{mantzilas_divinites_2000,
	type = {These de doctorat},
	title = {Les divinités dans l'œuvre poétique d'{Ovide}},
	copyright = {Licence Etalab},
	url = {https://theses.fr/2000PA040080},
	abstract = {Cette thèse contient une étude minutieuse de tous les aspects des divinités grecques, romaines, italiques et orientales chez Ovide. Le classement des divinités est fait selon leur fonction principale (les divinités du ciel, des montagnes et des champs, de la mer, des eaux douces, du feu et du métal, de la demeure, de la naissance de l'amour et du mariage, du vent, de la fécondité, divinités chthoniennes, orientales, de la médecine, de la guerre et de la paix, du temps, issues de Jupiter, les abstractions personnifiées, les vertus romaines, les héros divinisés de Rome). La présentation de chaque entité est divisée en deux parties : son culte à Rome et sa présentation dans l'ensemble de l'œuvre poétique d’Ovide (métamorphoses, fasti, ars amatoria, amores, remedia amoris, heroides, ibis, tristia, pontica, de medicamina faciei femmeae, fragmenta). Les thèmes concernant l'étude des divinités sont relatifs à la fois à la tradition mythologique grecque (mythes et légendes, anthropomorphisme, fonctions générales, symboles, interaction avec les hommes et les autres dieux, prières, invocations, vers magiques, aspects et hypostases, attributs, animaux et plantes sacrées) et à leur culte à Rome (origines et étiologie de leur culte, étymologie de leur nom, épithètes et épiclèses, temples, fêtes, rites, cérémonies, jeux et sacrifices en leur honneur, iconographie, liaison avec d'autres divinités dans le culte). En parallèle, on cherche à relier toutes ses informations avec les calendriers épigraphiques et les autres auteurs grecs et latins contemporains ou non.},
	urldate = {2025-01-11},
	school = {Paris 4},
	author = {Mantzilas, Dimitrios},
	collaborator = {André, Jean-Marie},
	month = jan,
	year = {2000},
	keywords = {Dieux -- Dans la littérature, notion, Ovide (-0043-0017) -- Religion},
}

